<!--
  Parent Rule for Windows DNS Server Analytical Events

  Channel: Microsoft-Windows-DNS-Server/Analytical

  Required:
  - wevtutil sl Microsoft-Windows-DNS-Server/Analytical /e:true

  IMPORTANT: This is a SEPARATE service from "dns-server" which uses
  the "DNS Server" channel (not the Analytical channel).

  Rule ID: 109991
-->

<group name="windows,dns-server-analytic,">

  <rule id="109991" level="3">
    <field name="win.system.channel">Microsoft-Windows-DNS-Server/Analytical</field>
    <description>Windows DNS Server Analytical Event</description>
    <options>no_full_log</options>
    <group>windows,dns-server-analytic,</group>
  </rule>

</group>
